package FIZZ.BUZZ;

public class index {

	public static void main(String[] args) {
		System.out.println("INGRESE UN NUMERO ENTERO ENTRE EL 1 Y EL 100");
		for (int i = 1; i <= 100; i++) {
            if (i % 15 == 0) {
                System.out.println("fizzbuzz");
            } else if (i % 3 == 0) {
                System.out.println("fizz");
            } else if (i % 5 == 0) {
                System.out.println("buzz");
            } else {
                System.out.println(i);
            }

	}

	}
}
